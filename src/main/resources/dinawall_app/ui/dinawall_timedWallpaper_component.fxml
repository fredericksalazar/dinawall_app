<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox alignment="CENTER_LEFT" fillHeight="false" prefHeight="67.0" prefWidth="704.0" spacing="10.0" style="-fx-background-color: #ced4da;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TextField fx:id="tfImageUrl" editable="false" prefHeight="26.0" prefWidth="348.0" promptText="Imagen URL" />
      <Button mnemonicParsing="false" onAction="#showFileDialog" prefHeight="26.0" prefWidth="97.0" text="Add Image" />
      <ComboBox fx:id="cbHour" prefHeight="26.0" prefWidth="88.0" promptText="Hour" />
      <ComboBox fx:id="cbMinutes" prefHeight="26.0" prefWidth="91.0" promptText="Minute" />
      <VBox prefHeight="60.0" prefWidth="39.0" spacing="5.0">
         <HBox.margin>
            <Insets />
         </HBox.margin>
         <padding>
            <Insets bottom="5.0" top="5.0" />
         </padding>
      </VBox>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets left="10.0" right="10.0" />
   </padding>
</HBox>
